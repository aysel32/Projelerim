import random
import copy

#verilen listedeki elemanların sırasını rastgele bir şekilde yazdırarak, her elemanın yalnızca bir kez yazdırılmasını sağlar

liste = ["Aysel", "Cengiz", "Emine", "Filiz", "Gülsüm", "Gülşah", "Keziban", "Onur Çıkıt", "Şerife", "Zeynep"]
kopya_liste = copy.deepcopy(liste)

for i in range(len(liste)):
    rastgele_index = random.randint(0, len(kopya_liste)-1)
    print(kopya_liste[rastgele_index])
    kopya_liste.pop(rastgele_index)
    
    


